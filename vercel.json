{
  "redirects": [
    { "source": "/vida-laboral", "destination": "/tse", "permanent": true },
    { "source": "/vida-laboral/:path*", "destination": "/tse", "permanent": true },
    { "source": "/informe-dgt", "destination": "/tse", "permanent": true },
    { "source": "/informe-dgt/:path*", "destination": "/tse", "permanent": true },
    { "source": "/nota-simple", "destination": "/tse", "permanent": true },
    { "source": "/nota-simple/:path*", "destination": "/tse", "permanent": true },
    { "source": "/:legacy(vida|laboral|dgt|nota|itv)/:path*", "destination": "/tse", "permanent": true }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
